{
  "name": "discord-irc",
  "version": "0.1.0",
  "description": "Connects IRC and Discord channels by sending messages back and forth.",
  "keywords": [
    "discord",
    "irc",
    "gateway",
    "bot",
    "discord-irc"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ekmartin/discord-irc.git"
  },
  "bugs": {
    "url": "https://github.com/ekmartin/discord-irc/issues"
  },
  "scripts": {
    "start": "node index.js",
    "lint": "eslint . --ignore-path .gitignore",
    "coverage": "istanbul cover _mocha $(find test -name '*.test.js') && istanbul report cobertura",
    "test": "npm run lint && npm run coverage"
  },
  "bin": {
    "discord-irc": "./index.js"
  },
  "author": {
    "name": "Martin Ek <mail@ekmartin.no>"
  },
  "license": "MIT",
  "dependencies": {
    "check-env": "~1.2.0",
    "commander": "~2.8.1",
    "irc": "~0.3.9",
    "lodash": "~3.9.3",
    "winston": "~1.0.0"
  },
  "devDependencies": {
    "chai": "~2.3.0",
    "discord.js": "~3.8.2",
    "eslint": "~1.6.0",
    "eslint-config-airbnb": "~0.1.0",
    "istanbul": "~0.3.5",
    "jscs": "~1.13.1",
    "jshint": "~2.7.0",
    "mocha": "~2.2.5",
    "rewire": "~2.3.1",
    "sinon": "~1.14.1",
    "sinon-chai": "~2.7.0"
  }
}
